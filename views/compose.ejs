<%- include("partials/header") %>

    <h1>Compose</h1>
    <br>

    <form action="/compose" method="post">

        <label for="input1" class="form-label">Title</label>
        <input type="text" name="composeTitle" class="form-control" id="input1" placeholder="Enter your title here">
        <br>
        <label for="input2" class="form-label">Post</label>
        <textarea type="text" name="composeText" class="form-control" id="input2" rows="12" placeholder="Compose Something"></textarea>
        <br>
        <button class="btn btn-success btn-lg" type="submit" name="button">Publish</button>

    </form>

<%- include("partials/footer") %>